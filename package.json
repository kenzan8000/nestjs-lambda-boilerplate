{
  "name": "nestjs-lambda-boilerplate",
  "version": "0.0.0",
  "description": "nestjs+lambda project boilerplate",
  "author": "kenzan8000",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "local": "docker-compose -f docker-compose.yml -f local.yml up",
    "stop": "docker-compose down",
    "swagger": "docker-compose -f docker-compose-swagger.yml -f local.yml up",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "docker-compose -f docker-compose-test.yml -f local.yml up; docker-compose down",
    "test:local": "TEST=true jest",
    "test:e2e": "docker-compose -f docker-compose-test-e2e.yml -f local.yml up; docker-compose down",
    "test:e2e:local": "TEST=true jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.12",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.12",
    "@nestjs/platform-fastify": "^7.6.12",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "@webundsoehne/nest-fastify-file-upload": "^1.0.0",
    "aws-sdk": "^2.842.0",
    "aws-serverless-fastify": "^1.0.25",
    "dayjs": "^1.10.4",
    "fastify": "^3.11.0",
    "fastify-multipart": "^4.0.0",
    "ip-address": "^7.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nestjs-i18n": "^8.0.11",
    "nestjs-rate-limiter": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "serverless-api-gateway-throttling": "^1.1.0",
    "serverless-layers": "^2.3.3",
    "serverless-provisioned-concurrency-autoscaling": "^1.2.0",
    "typeorm": "^0.2.31",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.12",
    "@types/aws-lambda": "^8.10.72",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/request-ip": "^0.0.35",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "fastify-swagger": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "serverless-domain-manager": "^5.1.0",
    "serverless-dotenv-plugin": "^3.3.0",
    "serverless-newrelic-lambda-layers": "^1.1.2",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-datadog": "^2.16.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.5",
    "yarn": "^1.22.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
